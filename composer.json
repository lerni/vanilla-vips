{
    "name": "silverstripe/installer",
    "type": "silverstripe-recipe",
    "description": "The SilverStripe Framework Installer",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/lerni/silverstripe-focuspoint"
        }
    ],
    "require": {
        "php": ">=7.1 <=8.3",
        "silverstripe/recipe-plugin": "^2",
        "silverstripe/recipe-cms": "^5",
        "silverstripe-themes/simple": "~3.3.0",
        "silverstripe/login-forms": "^5",
        "silverstripe/asset-admin": "2.x-dev",
        "jonom/silverstripe-betternavigator": "^6",
        "jonom/focuspoint": "dev-master",
        "jcupitt/vips": "^v2",
        "osiemsiedem/intervention-image-vips-driver": "^v0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "extra": {
        "resources-dir": "_resources",
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mimevalidator.yml",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "web.config"
        ]
    },
    "config": {
        "process-timeout": 600,
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/recipe-plugin": true,
            "silverstripe/vendor-plugin": true
        }
    },
    "autoload": {
		"psr-4": {
			"App\\": "app/src"
		}
	},
    "prefer-stable": true,
    "minimum-stability": "dev"
}
